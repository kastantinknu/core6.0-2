@model IEnumerable<zabolotnyi_zpi_zp_92_lab_kpi_3.Models.Player>

@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
<meta name="viewport " content="width=device-width" />
<title>Players</title>
   <link rel="stylesheet" href="/node_modules/bootstrap/dist/css/bootstrap.css"/>

</head>
<body>
<div class="panel-body">
<h2>Here is the list of players</h2>
<table class="table table-sm table-striped table-bordered">
<thead>
<tr>
<th>Id</th>
<th>Name</th>
<th>Position</th>
<th>RedCard</th>
</tr>
</thead>
<tbody>
@foreach ( zabolotnyi_zpi_zp_92_lab_kpi_3.Models.Player p in Model) {
<tr>
<td>@p.Id</td>
<td>@p.Name</td>
<td>@p.Position</td>
<td>@p.RedCard</td>
</tr>
}
</tbody>
</table>
</div>
</body>
</html>